{
    "reporterEnabled": "spec, mocha-junit-reporter, mochawesome",
	"mochaJunitReporterReporterOptions": {
		"mochaFile": "cypress/tests_results/junit/results-[hash].xml",
		"toConsole": false,
		"outputs": true,
		"testCaseSwitchClassnameAndName": true
	},
	"reporterOptions": {
        "reportDir": "cypress/tests_results/mocha",
        "overwrite": false,
        "html": false,
        "json": true
    }

}